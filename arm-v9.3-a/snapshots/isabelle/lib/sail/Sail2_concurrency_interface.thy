\<comment> \<open>\<open> val try_catchR : forall 'rv 'a 'r 'e1 'e2. monadR 'rv 'a 'r 'e1 -> ('e1 -> monadR 'rv 'a 'r 'e2) ->  monadR 'rv 'a 'r 'e2 \<close>\<close>
definition try_catchR  :: \<open>('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'l)sum))monad \<Rightarrow>('l \<Rightarrow>('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'b)sum))monad)\<Rightarrow>('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'b)sum))monad \<close>  where 
     \<open> try_catchR m h = (
  try_catch m
    ((\<lambda>x .  (case  x of   Inl r => throw (Inl r) | Inr e => h e ))))\<close> 
  for  "m"  :: "('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'l)sum))monad " 
  and  "h"  :: " 'l \<Rightarrow>('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'b)sum))monad "


