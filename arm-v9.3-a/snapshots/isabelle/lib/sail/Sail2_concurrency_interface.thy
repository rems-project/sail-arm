     \<open> catch_early_return m = (
  try_catch m
    ((\<lambda>x .  (case  x of   Inl a => return a | Inr e => throw e ))))\<close> 
  for  "m"  :: "('j,'i,'h,'g,'f,'e,'d,'c,'a,(('a,'b)sum))monad "


