definition early_return  :: \<open> 'a \<Rightarrow>('k,'j,'i,'h,'g,'f,'e,'d,'c,(('a,'b)sum))monad \<close>  where 
     \<open> early_return r = ( throw (Inl r))\<close> 
  for  "r"  :: " 'a "


